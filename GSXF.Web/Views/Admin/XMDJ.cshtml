
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>XMDJ</title>

    @Styles.Render("~/Content/easyui")
    @Scripts.Render("~/Content/easyuijs")
</head>
<body style=" margin:0px;">
    <div class="easyui-panel" title="技术服务项目备案">
        <div id="tb" style="height:auto">
            <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true" onclick="$('#dlg').dialog('open')" >新增</a>
            <a href="javascript:void(0)" class="easyui-linkbutton" iconCls="icon-edit" plain="true">修改</a>
            <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-remove',plain:true" >删除</a>
            <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-undo',plain:true">撤销</a>
        </div>
        <fieldset>
            <legend>查询信息</legend>
            项目名称：
            <input class="easyui-textbox">
            所属区县：
            <input class="easyui-textbox">
            <a href="#" class="easyui-linkbutton" iconCls="icon-search">查询</a>
        </fieldset>
        <table class="easyui-datagrid" pagination="true">
            <thead>
                <tr>
                    <th field="t1"  width="150px">项目名称</th>
                    <th field="t2" width="60px">服务类型</th>
                    <th field="t3" width="80px">所属区县</th>
                    <th field="t4" width="80px">项目负责人</th>
                    <th field="t5" width="120px">建设单位</th>
                    <th field="t6" width="150px">合同编号</th>
                    <th field="t7" width="80px">合同签订日期</th>
                    <th field="t8" width="160px">合同起止时间</th>
                    <th field="t9" width="60px">项目状态</th>
                    <th field="t10" width="80px">提交日期</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>上海汽车工业大厦</td>
                    <td>维修</td>
                    <td>黄浦</td>

                    <td>万建</td>
                    <td>上海汽车集团股份有限公司</td>
                    <td>SC15-B-0085</td>
                    <td>2015-01-01</td>
                    <td>2015-01-01/2015-12-31</td>
                    <td>已撤销</td>
                    <td>2015-07-09</td>
                </tr>
            </tbody>
        </table>
        <div id="dlg" class="easyui-dialog" title="项目信息" data-options="iconCls:'icon-save'" style="width:600px;height:400px;padding:0px;" closed="true">
            <div id="tabs" class="easyui-tabs" border="false" style="width:100%;height:100%;">
                <div title="项目信息" >
                    <table style="width:100%;height:100%;">
                        <tr>
                            <td>服务类型：</td>
                            <td>
                                <select class="easyui-combobox" name="Type" style="width:100px;"
                                        data-options="panelHeight:'auto'">
                                    <option value="0">竣工检测</option>
                                    <option value="1">年度检测</option>
                                    <option value="2">维护保养</option>
                                    <option value="3">安全评估</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <td>项目名称：</td>
                            <td><input class="easyui-textbox"></td>
                            <td>项目负责人</td>
                            <td>
                                <select class="easyui-combobox" name="state" style="width:80px;"
                                        data-options="panelHeight:'auto'">
                                    <option value="0">张三</option>
                                    <option value="1">李四</option>
                                    <option value="2">王五</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <td>项目地址</td>
                            <td colspan="3"><input class="easyui-textbox"></td>
                        </tr>
                        <tr>
                            <td>建设单位：</td>
                            <td><input class="easyui-textbox"></td>
                            <td>联系电话：</td>
                            <td><input class="easyui-textbox"></td>
                        </tr>
                        <tr>
                            <td>建设单位联系人：</td>
                            <td><input class="easyui-textbox"></td>
                            <td>联系手机</td>
                            <td><input class="easyui-textbox"></td>
                        </tr>
                        <tr>
                            <td>消防设施施工单位：</td>
                            <td colspan="3"><input class="easyui-textbox"></td>
                        </tr>
                        <tr>
                            <td>服务内容：</td>
                            <td colspan="3">
                                <select class="easyui-combobox" name="state" style="width:120px;"
                                        data-options="panelHeight:'auto'">
                                    <option value="0">内容1</option>
                                    <option value="1">内容2</option>
                                    <option value="2">内容3</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <td>最大单体建筑面积：</td>
                            <td><input class="easyui-textbox">m2</td>
                        </tr>
                        <tr>
                            <td>属于（包含）工业建筑：</td>
                            <td>
                                <input name="test" type="radio" value="" checked />是
                                <input name="test" type="radio" value="" />否
                            </td>
                        </tr>
                        <tr>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                        </tr>
                        <tr>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                        </tr>

                    </table>
                </div>
                <div title="信息变更" style="padding: 0px; overflow: hidden;">

                </div>
            </div>
        </div>
    </div>
</body>
</html>
