
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CXXM</title>

    @Styles.Render("~/Content/easyui")
    @Scripts.Render("~/Content/easyuijs")
</head>
<body>
    <fieldset style="border-width:1px">
        <legend>查询信息</legend>
        项目类型:
        <select class="easyui-combobox" name="state" style="width:102px;"
                data-options="panelHeight:'auto'" editable="false">
            <option value="0">全部</option>
            <option value="1">竣工检测</option>
            <option value="2">年度检测</option>
            <option value="3">维护保养</option>
            <option value="4">安全评估</option>
        </select>
        所属地区:
        <select class="easyui-combobox" name="state" style="width:80px;"
                data-options="panelHeight:'102px'" editable="false">
            <option value="0">全部</option>
            <option value="0">兰州</option>
            <option value="1">天水</option>
            <option value="2">金昌</option>
            <option value="3">白银</option>
            <option value="4">武威</option>
            <option value="5">平凉</option>
            <option value="6">酒泉</option>
            <option value="7">庆阳</option>
            <option value="8">定西</option>
            <option value="9">陇南</option>
            <option value="10">临夏</option>
            <option value="11">嘉峪关</option>
            <option value="12">甘南</option>
            <option value="13">张掖</option>
            <option value="14">兰州新区</option>
        </select>
        报告备案时间:
        <input class="easyui-datebox" style="width:96px;" editable="false">-<input class="easyui-datebox" style="width:96px;" editable="false">
        检测结果:
        <select class="easyui-combobox" name="state" style="width:62px;"
                data-options="panelHeight:'auto'" editable="false">
            <option value="0">全部</option>
            <option value="1">合格</option>
            <option value="2">不合格</option>
        </select>
        项目名称:
        <input class="easyui-textbox" style="width:120px" />
        <a href="#" class="easyui-linkbutton" iconCls="icon-search">查询</a>
    </fieldset>
    <table class="easyui-datagrid" pagination="true" data-options="url:'/Data/MyProjects'">
        <thead>
            <tr>
                <th field="Name" width="300px">项目名称</th>
                <th field="Type" width="100px">项目类型</th>
                <th field="City" width="80px">所属地区</th>
                <th field="RecordDate" width="96px">报告备案时间</th>
                <th field="qq" width="80px">检测结果</th>
            </tr>
        </thead>
        
    </table>
</body>
</html>
